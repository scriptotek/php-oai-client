    "keywords": ["OAI-PMH"],
    "require": {
        "danmichaelo/quitesimplexmlelement": "~0.2",
        "guzzle/guzzle": "~3.8"
    },
    "require-dev": {
        "php": ">=5.3",
        "phpunit/phpunit": "~4.0",
        "satooshi/php-coveralls": "~0.6",
        "mockery/mockery": "~0.9",
        "sami/sami": "~1.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Michael O. Hegg√∏",
            "email": "danmichaelo@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/scriptotek/oai-client/issues"
    },
    "autoload": {
        "psr-4": {
            "Scriptotek\\Oai\\": "src/"
        }
